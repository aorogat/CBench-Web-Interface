{{#hybrid.local}}
<tr> <td>{{id}}</td> <td>{{recall}}</td> <td>{{precision}}</td> <td>{{f1measure}}</td> </tr>
{{/hybrid.local}}
</table>
</p>
{{/hybrid.empty}}

